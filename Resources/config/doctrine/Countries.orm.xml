<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CPANA\ClassifiedsBundle\Entity\Countries" table="countries">
    <id name="idCountry" type="integer" column="id_country">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="countryCode" type="string" column="country_code" length="2" nullable="false"/>
    <field name="countryName" type="string" column="country_name" length="50" nullable="false"/>
  </entity>
</doctrine-mapping>
